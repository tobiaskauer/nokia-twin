ill\" style=\"height: 15px; padding-left: 10px\" aria-hidden=\"true\"></b-icon>\n      </b-button>\n      <b-collapse visible id=\"collapse-2\">\n      <div class=\"row elements\">\n        <div class=\"col\">\n          <ul> <!-- TO DO: use dynamic components and iterate over them to not repeat yourself -->\n            <li class=\"green\" v-for=\"element in elements\" v-bind:class=\"{highlight:element.key == selected0}\" :key=\"element.key\" @click=\"click(0,element.key)\">{{element.display}}</li>\n          </ul>\n        </div>\n        <div class=\"col\">\n          <ul>\n            <li class=\"purple\" v-for=\"element in elements\" v-bind:class=\"{highlight:element.key == selected1}\" :key=\"element.key\" @click=\"click(1,element.key)\">{{element.display}}</li>\n          </ul>\n        </div>\n      </div>\n      <input class=\"form-control form-control-sm\" type=\"text\" placeholder=\"Find more companies\">\n      </b-collapse>\n    </div>\n</template>\n\n<script>\n\n  export default {\n    data() {\n      return {\n        selected0: undefined,\n        selected1: undefined,\n        elements: [ //static for now, fed by db or files later.\n          {key: \"apple\", display: \"Apple\"},\n          {key: \"amazon\", display: \"Amazon\"},\n          {key: \"nokia\", display: \"Nokia\"},\n          {key: \"ericsson\", display: \"Ericsson\"},\n          {key: \"macys\", display: \"Macy's\"},\n          {key: \"mcdonalds\", display: \"McDonald's\"},\n          /*{key: \"huawei\", display: \"Huawei\"},\n          {key: \"starbucks\", display: \"Starbucks\"},\n          {key: \"telefonica\", display: \"Telefonica\"}*/\n        ]\n      }\n    }, methods:  {\n      click: function(n,key) {\n        if (n == 0) this.selected0 = key\n        if (n == 1) this.selected1 = key //TODO: this would have been so much prettier as an array\n        this.$emit(\"clicked\",{queryID: n,attr: \"company\", value: key}) //pass this to sidebar, which will then build the query\n      }\n    }\n  }\n  </script>\n"],"sourceRoot":"src/components"}]}